{
  "name": "aos-helper",
  "displayName": "AOS Helper",
  "description": "Provides suggestions for AOS animations and attributes in HTML, CSS, JavaScript, JSX, and TSX files.",
  "version": "1.2.1",
  "publisher": "nextsaimon",
  "engines": {
    "vscode": "^1.60.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nextSaimon/AOS-Helper.git"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:aos-helper.insertAOS",
    "onLanguage:html",
    "onLanguage:css",
    "onLanguage:javascript",
    "onLanguage:javascriptreact",
    "onLanguage:typescriptreact"
  ],
  "main": "./dist/extension.js",
  "icon": "image/icon.png",
  "contributes": {
    "commands": [
      {
        "command": "aos-helper.insertAOS",
        "title": "Insert AOS Basic Structure"
      }
    ],
    "languages": [
      {
        "id": "html",
        "aliases": [
          "HTML"
        ]
      },
      {
        "id": "css",
        "aliases": [
          "CSS"
        ]
      },
      {
        "id": "javascript",
        "aliases": [
          "JavaScript"
        ]
      },
      {
        "id": "javascriptreact",
        "aliases": [
          "JavaScript React"
        ]
      },
      {
        "id": "typescriptreact",
        "aliases": [
          "TypeScript React"
        ]
      },
      {
        "id": "astro",
        "aliases": [
          "Astro"
        ]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./"
  },
  "devDependencies": {
    "typescript": "^4.5.2",
    "@types/vscode": "^1.60.0"
  }
}
